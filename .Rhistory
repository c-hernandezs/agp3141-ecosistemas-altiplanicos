# En la consola de R, ejecuta:
list.files(pattern = "quarto", all.files = TRUE)
unlink(".quarto", recursive = TRUE)
# Verifica si existe
dir.exists(".quarto")
# Elimínala
unlink(".quarto", recursive = TRUE, force = TRUE)
# Verifica que se eliminó
dir.exists(".quarto")
dir.create("C:/temp/quarto_cache", recursive = TRUE, showWarnings = FALSE)
# Crea el archivo
writeLines("DENO_DIR=C:/temp/quarto_cache", ".Renviron")
# Verifica que se creó
file.exists(".Renviron")
